---
pagination:
  data: collections.linkstagList
  size: 1
  alias: tag
  filter:
    - all
    - nav
    - post
    - posts
    - linkstagList
  addAllPagesToCollections: true
layout: base.njk
eleventyComputed:
  title: Tagged “{{ tag }}”
permalink: /links/{{ tag | slug }}/
---
