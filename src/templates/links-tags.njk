<img src="https://via.placeholder.com/158" alt="">
<div class="row">
	<div class="column">
	<br>
	<br>
	<br>
	aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaassssssssssssssaaaaaaaaaa
		<h1>Tagged “{{ tag }}”</h1>

		{% set linkslist = collections.links %}
		{% set linkstagList = collections.linkstagList %}

		{% for link in linkslist %}
			{% for tagtest in linkstagList%}
				{% if tagtest === tag %}
					<div class="">
						{{tag}}{{link.data.title}}
					</div>
				{% endif %}
			{% endfor %}	
		{% endfor %}
		ende
		{% include "linkslist.njk" %}

		<p>
			See <a class="link-nav link-nav--helike" href="{{ '/links/' | url }}"><span>all tags</span></a>.
		</p>
	</div>
</div>
